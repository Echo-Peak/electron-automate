{
  "name": "electron-automate",
  "version": "0.0.8",
  "github": "",
  "scripts": {
    "start": "./node_modules/.bin/gulp.cmd init",
    "packager": "node scripts/packager.js",
    "installer": "node scripts/installer.js",
    "test": "./node_modules/.bin/mocha.cmd ./tests/index.js",
    "interface": "node ./command-interface.js",
    "inject-node": "node ./scripts/inject-node.js",
    "rename-node-alias": "node ./scripts/rename-node.js",
    "generate-keys": "node ./scripts/generate-keys.js --self",
    "post-install": "concurrently \"npm run inject-node\" \"npm run generate-keys\"",
    "postinstall": "npm run post-install",
    "postuninstall": "node ./scripts/remove-alias.js"
  },
  "author": "michael wheeler",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cookie-parser": "*",
    "electron": "^1.4.3",
    "electron-packager": "*",
    "express": "^4.14.0",
    "fs-extra": "*",
    "jade": "*",
    "material-ui": "*",
    "mkdirp": "*",
    "mobx": "^2.6.0",
    "mobx-react": "^3.5.8",
    "morgan": "*",
    "node-json-db": "*",
    "node-wmi": "*",
    "react": "*",
    "react-dom": "*",
    "react-motion": "*",
    "react-router": "^2.8.1",
    "react-swipeable-views": "^0.7.10",
    "react-tap-event-plugin": "^1.0.0",
    "robotjs": "*",
    "socket.io": "*",
    "socket.io-client": "*"
  },
  "devDependencies": {
    "gulp": "*",
    "gulp-concat": "*",
    "gulp-jade": "*",
    "gulp-sass": "*",
    "gulp-watch": "*",
    "gulp-rename": "*",
    "gulp-plumber": "*",
    "browser-sync": "*",
    "socket.io": "*",
    "socket.io-client": "*",
    "electron-packager": "*",
    "electron-prebuilt": "*",
    "electron-windows-installer": "*",
    "webpack": "*",
    "mocha": "*",
    "chai": "*",
    "node-wmi": "*",
    "colors": "*",
    "concurrently": "*",
    "mkdirp": "*"
  },
  "electron_packager": {
    "dir": "./built",
    "out": "./releases/packages",
    "name": "electron-automate",
    "version": "1.3.5",
    "prune": true,
    "overwrite": true,
    "asar": false,
    "icon": "./built/icon.ico",
    "win32metadata": {
      "FileDescription": "automate windows through the web",
      "CompanyName": "echo-peak",
      "OriginalFilename": "electron-automate",
      "InternalName": "echo-peak"
    }
  },
  "electron_installer": {
    "src": "./releases/packages",
    "out": "./installers"
  }
}
